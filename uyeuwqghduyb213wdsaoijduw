repeat wait() until game.Players.LocalPlayer.Backpack:FindFirstChild("CloverThrowingStars")
game.Players.LocalPlayer.Backpack:FindFirstChild("CloverThrowingStars").Parent = game.Players.LocalPlayer.Character
wait(.75)
Loops.clicked = true
repeat wait()
MouseButton1Click()
until not Loops.clicked
wait(.5)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-62.41, 33.38, 148.05)
wait(1)
Loops.clicked = false
end)
